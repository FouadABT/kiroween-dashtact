{
  "name": "fullstack-template",
  "version": "1.0.0",
  "description": "Full-stack template with Next.js frontend and NestJS backend",
  "scripts": {
    "setup": "node setup-workspace.js",
    "setup:fresh": "node setup-cli.js",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build"
  },
  "keywords": ["nextjs", "nestjs", "typescript", "postgresql", "prisma", "tailwind"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "pg": "^8.16.3"
  }
}